    "query": {
        "from": {
            "table": "System\\Routes\\Permissions\\Code\\Tables\\Permissions",
            "alias": "srp",
            "name": "#__system_routes_permissions"
        },
        "select": [
            "srp.id",
            "srp.role_id",
            "ur.alias AS role_id_alias",
            "srp.route_id",
            "sr.unique_name AS route_id_unique_name",
            "srp.can_add",
            "srp.can_view",
            "srp.can_write",
            "srp.can_delete",
            "srp.can_viewown",
            "srp.can_writeown",
            "srp.can_deleteown",
            "srp.created_by",
            "srp.date_created",
            "srp.modified_by",
            "srp.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Users\\Roles\\Code\\Tables\\Roles",
                "alias": "ur",
                "name": "#__users_roles",
                "on": "srp.role_id = ur.id"
            },
            {
                "table": "System\\Routes\\Code\\Tables\\Routes",
                "alias": "sr",
                "name": "#__system_routes",
                "on": "srp.route_id = sr.id"
            }
        ]
    }
