
<div id="fb-root"></div>

<script>(function (d, s, id) {
        var js, fjs = d.getElementsByTagName(s)[0];
        if (d.getElementById(id))
            return;
        js = d.createElement(s);
        js.id = id;
        js.src = "//connect.facebook.net/en_US/sdk.js#xfbml=1&version=v2.5&appId=214785215249084";
        fjs.parentNode.insertBefore(js, fjs);
    }(document, 'script', 'facebook-jssdk'));
</script>


<div class="fb-page" data-href="{{ href }}" data-width="{{ width }}" data-height="{{ height }}" data-small-header="{{ small_header }}" data-adapt-container-width="true" data-hide-cover="false" data-show-facepile="{{ show_facepile }}">
    <div class="fb-xfbml-parse-ignore">
        <blockquote cite="{{ href }}">
            <a href="{{ href }}">
                Facebook
            </a>
        </blockquote>
    </div>
</div>